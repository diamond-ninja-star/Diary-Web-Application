        <servlet-name>MemoryInsert</servlet-name>
        <url-pattern>/MemoryInsert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MemoryUpdate</servlet-name>
        <url-pattern>/MemoryUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VisitorCounterSession</servlet-name>
        <url-pattern>/VisitorCounterSession</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Welcome</servlet-name>
        <url-pattern>/Welcome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MemoryDelete</servlet-name>
        <url-pattern>/MemoryDelete</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
